@model IEnumerable<CoreBusiness.Product>

@if (Model != null && Model.Count() > 0)
{
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Product Name</th>
                <th>Quantity</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var prod in Model)
            {
                if (prod.Quantity > 0)
                {
                    <tr class="product-row" product-id="@prod.Id">
                        <td>@prod.Name</td>
                        <td>@prod.Quantity</td>
                    </tr>
                }
            }
        </tbody>
    </table>
}